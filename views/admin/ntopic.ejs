<%- include("../partials/header") %>



<div class="containerc">

  <div class="box glass col-12">

    <form method="post" class="col-12 text-light" onsubmit="return Password()">

      <div>
        <h3 style="font-weight: bolder;"><span style="color: #20d945;">new</span><span style="color: #858484;">topic</span></h3>
      </div>

      <div class="form-group my-3">
        <label>Topic</label>
        <input type="text" class="form-control" name="topicname" required>
      </div>

      <div class="form-group my-3">
        <label for="">Manager name</label>
        <input type="text" class="form-control" name="managername" required>
      </div>

      <div class="form-group my-3">
        <label for="">Email</label>
        <input type="text" class="form-control" name="email" required>
      </div>

      <div class="form-group my-3">
        <label for="">Password</label>
        <input type="password" class="form-control prePassword" name="prePassword" required>
      </div>

      <div class="form-group my-3">
        <label for="">Confirm Password</label>
        <input type="password" class="form-control password" name="password" required>
      </div>

      <div class="box text-light box-n messagebox col-12">
        <p id="message"></p>
      </div>

      <div class="centeralign">
        <button type="submit" class="btn btn-outline-light my-3 col-6">Submit</button>
      </div>

    </form>
  </div>

</div>





<%- include("../partials/footer") %>